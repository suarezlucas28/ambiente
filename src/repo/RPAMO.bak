	<queryString><![CDATA[SELECT
     pagos.`pag_codigo` AS pagos_pag_codigo,
     CONCAT(cliente.`cli_nomape`,' - ',obras.`obr_proyec`) AS obra,
     pagos.`pag_horfec` AS pagos_pag_horfec,
     pagos.`pag_monto` AS pagos_pag_monto,
     pagos.`pag_destin` AS pagos_pag_destin,
     pagos.`pag_observ` AS pagos_pag_observ,
     pagos.`pag_fecrec` AS pagos_pag_fecrec,
     usuarios.`usu_usuari` AS usuarios_usu_usuari
FROM
     `usuarios` usuarios INNER JOIN `pagos` pagos ON usuarios.`usu_codigo` = pagos.`usu_codigo`
     INNER JOIN `obras` obras ON usuarios.`usu_codigo` = obras.`usu_codigo`
     AND obras.`obr_codigo` = pagos.`obr_codigo`,
     `cliente` cliente
 WHERE cliente.cli_codigo = obras.cli_codigo AND pag_estado LIKE 'ACTIVO']]></queryString>

