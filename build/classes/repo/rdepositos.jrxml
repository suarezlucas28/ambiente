	<queryString><![CDATA[SELECT
     movimientos.`nro_recibo` AS movimientos_nro_recibo,
     movimientos.`cue_monto` AS movimientos_cue_monto,
     movimientos.`cue_fechor` AS movimientos_cue_fechor,
     cliente.`cli_nomape` AS cliente_cli_nomape,
     usuarios.`usu_usuari` AS usuarios_usu_usuari
FROM
     `obras` obras INNER JOIN `movimientos` movimientos ON obras.`obr_codigo` = movimientos.`obr_codigo`
     INNER JOIN `usuarios` usuarios ON movimientos.`usu_codigo` = usuarios.`usu_codigo`
     AND usuarios.`usu_codigo` = obras.`usu_codigo`,
     `cliente` cliente 
 WHERE obras.cli_codigo = cliente.cli_codigo AND cue_opera LIKE 'ENTREGA' AND cue_estado LIKE 'ACTIVO' ORDER BY nro_recibo DESC]]></queryString>

