	<queryString><![CDATA[SELECT
     obras.`obr_codigo` AS obras_obr_codigo,
     cliente.`cli_nomape` AS cliente_cli_nomape,
     obras.`obr_fecha` AS obras_obr_fecha,
     obras.`obr_proyec` AS obras_obr_proyec,
     obras.`obr_presup` AS obras_obr_presup,
     usuarios.`usu_usuari` AS usuarios_usu_usuari
