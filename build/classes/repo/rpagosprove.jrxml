	<queryString><![CDATA[SELECT
     paprove.`ppr_codigo` AS paprove_ppr_codigo,
     proveedor.`pro_nombre` AS proveedor_pro_nombre,
     paprove.`ppr_horfec` AS paprove_ppr_horfec,
     paprove.`ppr_monto` AS paprove_ppr_monto,
     obras.`obr_proyec` AS obras_obr_proyec,
     cliente.`cli_nomape` AS cliente_cli_nomape,
     usuarios.`usu_usuari` AS usuarios_usu_usuari
FROM
     `usuarios` usuarios INNER JOIN `paprove` paprove ON usuarios.`usu_codigo` = paprove.`usu_codigo`
     INNER JOIN `obras` obras ON usuarios.`usu_codigo` = obras.`usu_codigo`
     AND obras.`obr_codigo` = paprove.`obr_codigo`
     INNER JOIN `proveedor` proveedor ON paprove.`pro_codigo` = proveedor.`pro_codigo`,
     `cliente` cliente]]></queryString>

