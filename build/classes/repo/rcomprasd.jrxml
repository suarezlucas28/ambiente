	<queryString><![CDATA[SELECT
     compras.`com_codigo` AS compras_com_codigo,
     proveedor.`pro_nombre` AS proveedor_pro_nombre,
     CONCAT(cliente.`cli_nomape`,' - ',obras.`obr_proyec`) AS obras_obr_proyec,
     compras.`com_fechor` AS compras_com_fechor,
     usuarios.`usu_usuari` AS usuarios_usu_usuari,
	compras_detalle.ite_codigo,ite_descri,uni_descri,com_cantid
FROM
     `usuarios` usuarios INNER JOIN `compras` compras ON usuarios.`usu_codigo` = compras.`usu_codigo`
     INNER JOIN `obras` obras ON usuarios.`usu_codigo` = obras.`usu_codigo`
     AND obras.`obr_codigo` = compras.`obr_codigo`
     INNER JOIN `proveedor` proveedor ON compras.`pro_codigo` = proveedor.`pro_codigo`,
     `cliente` cliente,unidades,items,compras_detalle
WHERE obras.cli_codigo = cliente.cli_codigo AND compras.com_codigo = compras_detalle.com_codigo AND compras_detalle.ite_codigo = items.ite_codigo AND items.uni_precom = unidades.uni_codigo]]></queryString>

